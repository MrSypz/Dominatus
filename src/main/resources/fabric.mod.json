{
  "schemaVersion": 1,
  "id": "dominatus",
  "version": "${version}",
  "name": "Dominatus",
  "description": "RIF stands for 'Reduced Invincibility Frames.' The purpose of this mod is to reduce iFrame duration or disable it.",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/dominatus/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "sypztep.dominatus.client.DominatusClient"
    ],
    "main": [
      "sypztep.dominatus.Dominatus"
    ],
    "jade": [
      "sypztep.dominatus.common.compat.JadePlugin"
    ],
    "modmenu": [
      "sypztep.dominatus.common.compat.ModMenuIntegration"
    ],
    "cardinal-components-entity": [
      "sypztep.dominatus.common.init.ModEntityComponents"
    ],
    "fabric-datagen": [
      "sypztep.dominatus.client.data.ModDataGenerator"
    ]
  },
  "mixins": [
    "dominatus.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=1.21 <=1.21.1",
    "cloth-config2": ">=${cloth_config_version}",
    "accessories" : ">=${accessories_version}",
    "knumber" : ">=${knumber_veriosn}"
  },
  "accessWidener": "dominatus.accesswidener",
  "custom": {
    "cardinal-components": [
      "dominatus:failstack"
    ]
  }
